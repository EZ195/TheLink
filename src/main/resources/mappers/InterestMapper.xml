<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.ezone.link.user.interest.dao.InterestDAO">
	<insert id="addUserInterest" parameterType="com.ezone.link.user.interest.model.Interest">
		INSERT INTO `user_interest`
			(
				`userId`
				,`userInterest`
				,`createdAt`
			)
		VALUE
			(
				#{userId}
				,#{userInterest}
				,now()
			)
			
	</insert>

</mapper>